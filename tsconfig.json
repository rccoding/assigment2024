{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "allowJs": true
    },
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "paths": {
      "*": ["*", "src/*"]
    },
    "exclude": [
      "node_modules"
    ]
  }
  